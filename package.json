{
  "name": "tree-sitter-erlang",
  "version": "0.0.1",
  "description": "An Erlang grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "format": "prettier --trailing-comma es5 --write grammar.js",
    "format-check": "prettier --trailing-comma es5 --check grammar.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-mikedavis/tree-sitter-erlang.git"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "lexer",
    "erlang"
  ],
  "author": "the-mikedavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-mikedavis/tree-sitter-erlang/issues"
  },
  "homepage": "https://github.com/the-mikedavis/tree-sitter-erlang#readme",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "file-types": [
        "erl",
        "hrl",
        "rebar.config"
      ]
    }
  ]
}
